<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletLogOut</servlet-name>
        <servlet-class>Vista.ServletLogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Vista.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarUsuarioServlet</servlet-name>
        <servlet-class>Vista.AgregarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cargar</servlet-name>
        <servlet-class>Vista.Cargar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargaPlanServlet</servlet-name>
        <servlet-class>Vista.CargaPlanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SolicitarPlan</servlet-name>
        <servlet-class>Vista.SolicitarPlan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargarSolicitud</servlet-name>
        <servlet-class>Vista.CargarSolicitud</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalculosServlet</servlet-name>
        <servlet-class>Vista.CalculosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VolverServlet</servlet-name>
        <servlet-class>Vista.VolverServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarSolicitudes</servlet-name>
        <servlet-class>Vista.ListarSolicitudes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogOut</servlet-name>
        <url-pattern>/ServletLogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarUsuarioServlet</servlet-name>
        <url-pattern>/AgregarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cargar</servlet-name>
        <url-pattern>/Cargar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargaPlanServlet</servlet-name>
        <url-pattern>/CargaPlanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SolicitarPlan</servlet-name>
        <url-pattern>/SolicitarPlan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargarSolicitud</servlet-name>
        <url-pattern>/CargarSolicitud</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalculosServlet</servlet-name>
        <url-pattern>/CalculosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VolverServlet</servlet-name>
        <url-pattern>/VolverServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarSolicitudes</servlet-name>
        <url-pattern>/ListarSolicitudes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
